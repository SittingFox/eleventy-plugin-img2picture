    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo-space": "^0.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "image-size": "^0.9.7",
    "lint-staged": "^11.0.1",
